<?xml version="1.0" encoding="utf-8"?>
<rotate
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="0"
    android:pivotX="50%"
    android:pivotY="50%"
    android:toDegrees="360">
    <shape
        android:shape="ring"
        android:background="#65CDC9C9"
        android:thicknessRatio="9.9"
        android:useLevel="false">
        <size
            android:width="78dip"
            android:height="78dip" />
        <gradient
            android:angle="0"
            android:endColor="#00ffffff"
            android:startColor="#266128"
            android:type="sweep"
            android:useLevel="false" />
    </shape>
</rotate>